{
  "name": "assignment-fetcher",
  "displayName": "Assignment_Fetcher",
  "description": "fetch assignment from question repo and make branch in the users repo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:assignment-fetcher.helloWorld",
    "onCommand:assignment-fetcher.gitSet"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "assignment-fetcher.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "assignment-fetcher.gitSet",
        "title": "Fetch Assignment"
      }
    ],
    "configuration":[
      {
        "title": "Assignment Fetcher",
        "properties": {
          "Username":{
            "type": "string",
            "default": "",
            "description": "Enter github username"
          },
          "Password":{
            "type": "string",
            "default": "",
            "format": "password",
            "description": "Enter github password" 
          },
          "Lab Repository":{
            "type": "string",
            "default": "",
            "description": "Enter github link to Lab Repository" 
          },
          "Student Repository":{
            "type": "string",
            "default": "",
            "description": "Enter github link to Student Repository" 
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "simple-git": "^2.41.1"
  }
}
